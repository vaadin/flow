<% if (contentFor('jsMonkeyPatch')) { -%>
    // This patch is sent and executed each time the component is added/removed from
    // the UI, or when the View route changes.
    private static final String MONKEY_PATCH_JS =
<%- contentFor('jsMonkeyPatch').replace(/^\n+|\n+$/g, '').split('\n').map((line) => `            "${line}\\n"`).join(' +\n') -%>;

    public <%= name %>() {
        super();
        UI.getCurrent().getPage().executeJavaScript(MONKEY_PATCH_JS, this);
    }
<% } -%>
