<% locals.isInterface = false; -%>
<%- include('_disclaimer.java.ejs') -%>

<%- include('_package.java.ejs', {package: package}) -%>

<%- include('_common-imports.java.ejs') -%>
import com.vaadin.annotations.Tag;
import com.vaadin.ui.PropertyDescriptor;
import com.vaadin.ui.PropertyDescriptors;
<%- contentFor('imports') -%>

@HtmlImport("bower_components/<%= path %>")
@Tag("<%= is %>")
public class <%= name %> extends HtmlContainer {

<%- contentFor('classBodyStart') -%>

<%- include('_js-monkey-patch.java.ejs') -%>

<% if (!_.find(publicProperties, {name: 'text'})) { -%>
    public String getText() {
        return getElement().getText();
    }
    public void setText(String text) {
        getElement().setText(text);
    }

<% } -%>
<% if (publicProperties.length) { -%>

    /* Properties */
<% publicProperties.forEach(function(property) { %>
<%- include('_property.java.ejs', {property: property, name: name}) -%>
<% }) -%>
<% } -%>
<% if (publicMethods.length) { -%>

    /* Methods */
<% publicMethods.forEach(function(method) { %>
<%- include('_method.java.ejs', {method: method, name: name}) -%>
<% }); -%>
<% } -%>
<% if (events) { -%>

    /* Events */
<% events.forEach(function(event) { %>
<%- include('_event.java.ejs', {event: event, name: name}) -%>
<% }) -%>
<% } -%>

<%- contentFor('classBodyEnd') -%>

}
