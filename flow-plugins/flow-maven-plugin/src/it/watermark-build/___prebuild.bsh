// Make a copy of the main build log, so that verify can assert on
// output generated by a single execution.

import java.nio.file.*;
import java.util.*;

logFile = basedir.toPath().resolve("build.log");
if (Files.exists(logFile, new LinkOption[0])) {
    tempFile = logFile.resolveSibling("build-" + UUID.randomUUID().toString() + ".log");
    Files.move(logFile, tempFile, new CopyOption[0]);
    Files.writeString(logFile, "", new OpenOption[0]);
}
